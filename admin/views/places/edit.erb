<ul class="nav nav-tabs">
  <li> <%= link_to tag_icon(:list, pat(:list)), url(:places, :index) %></li>
  <li> <%= link_to tag_icon(:plus, pat(:new)), url(:places, :new) %></li>
  <li class="active"> <%= link_to tag_icon(:edit, pat(:edit)), url(:places, :edit, :id => @place.id) %></li>
</ul>
<div class="tabs-content">
  <% form_for :place, url(:places, :update, :id => @place.id), :method => :put, :class => 'form-horizontal' do |f| %>
    <%= partial 'places/form', :locals => { :f => f } %>
  <% end %>
</div>
